This is the user edit page.


<%= form_for @user do |f| %>
	<%= f.label :fname %>
	<%= f.text_field :fname %>
	<%= f.label :lname %>
	<%= f.text_field :lname %>
	<%= f.label :email %>
	<%= f.text_field :email %>
	<%= f.label :description %>
	<%= f.text_area :description %>
	<%= f.submit :update %>
<% end %>